{
  "name": "sc2pte-frontend",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.8.3",
    "@svgr/webpack": "5.1.0",
    "@testing-library/jest-dom": "5.0.0",
    "@testing-library/react": "9.4.0",
    "@testing-library/user-event": "8.0.3",
    "@types/classnames": "2.2.9",
    "@types/jest": "24.9.0",
    "@types/node": "13.1.8",
    "@types/react": "16.9.17",
    "@types/react-beautiful-dnd": "11.0.4",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.3",
    "@types/react-tabs": "2.3.1",
    "@types/react-timeago": "4.1.1",
    "@types/yup": "0.26.27",
    "@typescript-eslint/eslint-plugin": "2.17.0",
    "@typescript-eslint/parser": "2.17.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.5",
    "babel-preset-react-app": "9.1.0",
    "camelcase": "5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "classnames": "2.2.6",
    "css-loader": "3.4.2",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "5.0.2",
    "formik": "2.1.2",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-fetch-mock": "3.0.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "pre-commit": "1.2.2",
    "query-string": "6.10.1",
    "react": "16.12.0",
    "react-app-polyfill": "1.0.5",
    "react-beautiful-dnd": "12.2.0",
    "react-dev-utils": "10.0.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-stroller": "1.6.7",
    "react-tabs": "3.1.0",
    "react-timeago": "4.4.0",
    "react-twitch-ext-onauthorized": "2.0.1",
    "resolve": "1.14.2",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "7.1.1",
    "starcraft2-api": "1.3.3",
    "style-loader": "1.1.3",
    "swr": "0.1.16",
    "terser-webpack-plugin": "2.3.2",
    "ts-pnp": "1.1.5",
    "typeface-lato": "0.0.75",
    "typescript": "3.7.5",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.1",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "4.3.1",
    "yup": "0.28.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "dev": "npm start",
    "test": "jest",
    "test:watch": "node scripts/test.js",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint */**/*.{ts,tsx} --fix",
    "build": "npm run lint && npm test && node scripts/build.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "no-console": [
        "error"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "no-trailing-spaces": [
        "error"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 0
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "react/jsx-max-props-per-line": [
        "error",
        {
          "maximum": 1,
          "when": "always"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "./"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
