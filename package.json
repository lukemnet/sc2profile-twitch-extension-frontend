{
  "name": "sc2pte-frontend",
  "version": "2.0.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@babel/core": "7.9.6",
    "@svgr/webpack": "5.4.0",
    "@testing-library/jest-dom": "5.7.0",
    "@testing-library/react": "10.0.4",
    "@testing-library/user-event": "10.3.1",
    "@types/classnames": "2.2.10",
    "@types/jest": "25.2.2",
    "@types/node": "14.0.1",
    "@types/react": "16.9.35",
    "@types/react-beautiful-dnd": "12.1.2",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-tabs": "2.3.1",
    "@types/react-timeago": "4.1.1",
    "@types/yup": "0.28.3",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-named-asset-import": "0.3.6",
    "babel-preset-react-app": "9.1.2",
    "camelcase": "6.0.0",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "classnames": "2.2.6",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.0.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "5.0.3",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.2",
    "file-loader": "6.0.0",
    "formik": "2.1.4",
    "fs-extra": "9.0.0",
    "html-webpack-plugin": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-fetch-mock": "3.0.3",
    "jest-resolve": "26.0.1",
    "jest-watch-typeahead": "0.6.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.4",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "9.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.2",
    "pre-commit": "1.2.2",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-beautiful-dnd": "13.0.0",
    "react-dev-utils": "10.2.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.1.2",
    "react-stroller": "1.6.7",
    "react-tabs": "3.1.0",
    "react-timeago": "4.4.0",
    "react-twitch-ext-onauthorized": "2.0.1",
    "resolve": "1.17.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "7.3.2",
    "starcraft2-api": "1.3.4",
    "style-loader": "1.2.1",
    "swr": "0.2.0",
    "terser-webpack-plugin": "3.0.1",
    "ts-pnp": "1.2.0",
    "typeface-lato": "0.0.75",
    "typescript": "3.9.2",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.1.3",
    "yup": "0.28.5"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "dev": "npm start",
    "test": "jest",
    "test:watch": "node scripts/test.js",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint */**/*.{ts,tsx} --fix",
    "build": "npm run lint && npm test && node scripts/build.js"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "quotes": [
        "error",
        "single",
        {
          "avoidEscape": true
        }
      ],
      "indent": [
        "error",
        2
      ],
      "no-console": [
        "error"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "no-trailing-spaces": [
        "error"
      ],
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 1,
          "maxEOF": 0
        }
      ],
      "semi": [
        "error",
        "always"
      ],
      "react/jsx-max-props-per-line": [
        "error",
        {
          "maximum": 1,
          "when": "always"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "./"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
